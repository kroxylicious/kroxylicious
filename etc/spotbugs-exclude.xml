<FindBugsFilter>
    <!--
        The kroxylicious-kubernetes-api package-info.java uses @DefaultAnnotation(NonNull.class) like the other modules
        but default JSON values are usually specified using @io.fabric8.generator.annotation.Default;
        Null values are used to exclude properties from the final JSON e.g. in combination with
        @com.fasterxml.jackson.annotation.JsonInclude(com.fasterxml.jackson.annotation.JsonInclude.Include.NON_NULL)
        -->
    <Match>
        <Source name="~.*io/kroxylicious/kubernetes/api/common/.*"/>
        <Bug pattern="NP_NONNULL_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
    </Match>
    <!-- This is excluded because the classes are generated by sundrio / fabric8 -->
    <Match>
        <Class name="~io\.kroxylicious\.kubernetes\.api\.common\..+(Builder|Fluent)(\$.*)?" />
    </Match>
</FindBugsFilter>