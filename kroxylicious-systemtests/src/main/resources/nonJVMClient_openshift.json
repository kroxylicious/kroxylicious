{
  "spec": {
    "containers": [{
      "name": "%CONTAINER_NAME%",
      "securityContext": {
        "allowPrivilegeEscalation": false,
        "runAsNonRoot": true,
        "seccompProfile": {
          "type": "RuntimeDefault"
        },
        "capabilities": {
          "drop": [ "ALL" ]
        }
      }
    }]
  }
}
